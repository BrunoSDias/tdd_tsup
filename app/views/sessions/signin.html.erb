<h1>Sessions#signin</h1>

<%= form_for :session, url: sessions_login_path do |f| %>
  <%= f.label :email %>
  <%= f.text_field :email %>

  <%= f.label :password %>
  <%= f.password_field :password %>

  <%= f.submit 'Enter' %>
<% end %>



<%= link_to 'Create a new account', sessions_signup_path %>
